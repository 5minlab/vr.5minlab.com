<!DOCTYPE html>
<html lang="en">
  <head>
    <title>5minlab - 5분실험실</title>
    <meta charset="utf-8">

    <meta name="description" content="5minlab's VR/3D Corporate website">
    <meta name="keywords" content="vr,3d,webgl,webvr">
    <meta name="author" content="5minlab">

    <meta name="viewport" content="width=device-width, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0, shrink-to-fit=no">
    <meta name="mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent" />

    <style>
      body {
        background-color: #000;
        color: #FFF;
        margin: 0px;
        overflow: hidden;
      }
    </style>
  </head>
  <body>
    <div id="container"></div>

    <script>
      function getWebVRConfig() {
        var defaultValue = <%- JSON.stringify(WebVRConfig) %>;
        return defaultValue;
      }
      WebVRConfig = getWebVRConfig();
      console.log("WebVRConfig : " + JSON.stringify(WebVRConfig));
    </script>

    <% if (env.production) { %>
      <script src="<%= env.assets.vendor.js %>"></script>
      <script src="<%= env.assets.index.js %>"></script>
    <% } else { %>
      <script src="//<%= env.hostname %>:3000/dist/vendor.bundle.js"></script>
      <script src="//<%= env.hostname %>:3000/dist/index.bundle.js"></script>
    <% } %>

    <!-- google analytics -->
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-66505862-2', 'auto');
      ga('send', 'pageview');

    </script>
  </body>
</html>
